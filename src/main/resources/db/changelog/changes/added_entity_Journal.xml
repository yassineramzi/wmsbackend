<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <property name="autoIncrement" value="true"/>

    <!--
        Added the entity Journal.
    -->
    <changeSet id="03-11-2022-10-43" author="">
        <createTable tableName="journal">
            <column name="id" type="bigint" autoIncrement="${autoIncrement}">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="typeJournal" type="VARCHAR(255)"/>
            <column name="nomJournal" type="VARCHAR(255)"/>
            <column name="id_plan_comptable" type="bigint"/>
        </createTable>

    </changeSet>
</databaseChangeLog>
